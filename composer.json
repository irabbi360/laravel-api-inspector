{
    "name": "irabbi360/laravel-api-inspector",
    "description": "Laravel Auto Generate API Documentation for request rules, parameters and API Response",
    "keywords": [
        "Irabbi360",
        "laravel",
        "laravel-api-inspector",
        "api-documentation",
        "api-docs",
        "api-inspector",
        "api-tester"
    ],
    "homepage": "https://github.com/irabbi360/laravel-api-inspector",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Fazle Rabbi",
            "email": "fazle08rabbi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 | ^8.2 | ^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0 | ^11.0 | ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.8 | ^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Irabbi360\\LaravelApiInspector\\": "src/",
            "Irabbi360\\LaravelApiInspector\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Irabbi360\\LaravelApiInspector\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Irabbi360\\LaravelApiInspector\\LaravelApiInspectorServiceProvider"
            ],
            "aliases": {
                "LaravelApiInspector": "Irabbi360\\LaravelApiInspector\\Facades\\LaravelApiInspector"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}